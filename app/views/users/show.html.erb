<h1><%= @user.name %>'s Page</h1>

<div class="well well-small">
	<ul>
		<li>Hometown: <%= @user.hometown  %></li>
		<li>Been Climbing For <%= @user.climbingfor %></li>
		<li>Climbing Ability: <%= @user.ability %></li>
		<li>Favorite Climb: <%= @user.favclimb %></li>
		<li>Other Interests:<%= @user.interests %></li>
	</ul>
</div>


<div id="routes">
  <%= render @routes %>
</div>

<%= will_paginate @routes %>
